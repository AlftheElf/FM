<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
	..\FrameXML\UI.xsd">

	<Script file="FM.lua"/>
	<Frame name="FM" parent="UIParent" hidden="false">
		<Scripts>
			<OnLoad>
				FM_OnLoad();
			</OnLoad>
			<OnEvent>
				FM_OnEvent(event);
			</OnEvent>
			<OnUpdate>
				FM_OnUpdate();
			</OnUpdate>
		</Scripts>
	</Frame>
</Ui>
